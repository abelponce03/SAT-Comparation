{
  "minisat": {
    "name": "MiniSat",
    "version_patterns": ["2\\.2\\.0", "2\\.2"],
    "build_files": ["Makefile", "core/Makefile"],
    "build_commands": [
      "cd core && make"
    ],
    "executable_patterns": [
      "core/minisat",
      "minisat",
      "build/minisat"
    ],
    "test_command": "{executable} -h",
    "run_command": "{executable} {input} {output}",
    "timeout_flag": null,
    "output_parser": "minisat"
  },
  
  "cadical": {
    "name": "CaDiCaL",
    "version_patterns": ["1\\.\\d+\\.\\d+"],
    "build_files": ["configure", "makefile"],
    "build_commands": [
      "./configure",
      "make"
    ],
    "executable_patterns": [
      "build/cadical",
      "cadical"
    ],
    "test_command": "{executable} --version",
    "run_command": "{executable} {input} {output}",
    "timeout_flag": null,
    "output_parser": "cadical"
  },
  
  "glucose": {
    "name": "Glucose",
    "version_patterns": ["4\\.\\d+", "3\\.\\d+"],
    "build_files": ["Makefile", "core/Makefile"],
    "build_commands": [
      "cd core && make"
    ],
    "executable_patterns": [
      "core/glucose",
      "glucose",
      "simp/glucose"
    ],
    "test_command": "{executable} -h",
    "run_command": "{executable} {input} {output}",
    "timeout_flag": null,
    "output_parser": "glucose"
  },
  
  "cryptominisat": {
    "name": "CryptoMiniSat",
    "version_patterns": ["5\\.\\d+\\.\\d+"],
    "build_files": ["CMakeLists.txt"],
    "build_commands": [
      "mkdir -p build",
      "cd build && cmake ..",
      "cd build && make"
    ],
    "executable_patterns": [
      "build/cryptominisat5",
      "cryptominisat5"
    ],
    "test_command": "{executable} --version",
    "run_command": "{executable} {input} {output}",
    "timeout_flag": null,
    "output_parser": "cryptominisat"
  },
  
  "kissat": {
    "name": "Kissat",
    "version_patterns": ["3\\.\\d+\\.\\d+"],
    "build_files": ["configure"],
    "build_commands": [
      "./configure",
      "make"
    ],
    "executable_patterns": [
      "build/kissat",
      "kissat"
    ],
    "test_command": "{executable} --version",
    "run_command": "{executable} {input}",
    "timeout_flag": null,
    "output_parser": "kissat"
  },
  
  "lingeling": {
    "name": "Lingeling",
    "version_patterns": [".*"],
    "build_files": ["configure.sh", "makefile"],
    "build_commands": [
      "./configure.sh",
      "make"
    ],
    "executable_patterns": [
      "lingeling",
      "build/lingeling"
    ],
    "test_command": "{executable} -h",
    "run_command": "{executable} {input}",
    "timeout_flag": null,
    "output_parser": "lingeling"
  }
}
